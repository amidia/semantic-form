{
    "name": "laravolt/semantic-form",
    "description": "Semantic UI form helpers",
    "keywords": [
        "laravel",
        "laravolt",
        "form",
        "semantic UI"
    ],
    "homepage": "https://github.com/laravolt/semantic-form",
    "license": "MIT",
    "authors": [
        {
            "name": "Bayu Hendra Winata",
            "email": "bayu.hendra@javan.co.id",
            "homepage": "https://laravolt.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "5.6.*|5.7.*|5.8.*|6.*",
        "nesbot/carbon": "1.25.* || ^2.0",
        "jenssegers/date": "^3.4",
        "myclabs/deep-copy": "^1.9"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.2",
        "squizlabs/php_codesniffer": "^3.4",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "suggest": {
        "laravolt/ui": "Some semantic-form components only works in Laravolt"
    },
    "autoload": {
        "psr-4": {
            "Laravolt\\SemanticForm\\": "src"
        },
        "files": [
            "src\\helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Laravolt\\SemanticForm\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravolt\\SemanticForm\\ServiceProvider"
            ],
            "aliases": {
                "Form": "Laravolt\\SemanticForm\\Facade",
                "SemanticForm": "Laravolt\\SemanticForm\\Facade"
            }
        }
    }
}
